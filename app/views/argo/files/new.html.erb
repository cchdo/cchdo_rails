<div class="boxed">

<h1>
  New Argo file
  <%= render :partial => 'argo/session' %>
</h1>

<div class="box_content">

<% form_for(@file, :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :ExpoCode %><br />
    <%= f.text_field :ExpoCode %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :display %><br />
    <%= f.check_box :display %>
  </p>
  <p>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', argo_files_path %>

</div>
<div class="box_bottom"></div>
</div>
